{
  "name": "wdio-project",
  "author": {"name": "Hardik Chotaliya"},
  "version": "8.32.1",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.31.1",
    "@wdio/cli": "^8.32.1",
    "@wdio/cucumber-framework": "^8.32.1",
    "@wdio/local-runner": "^8.32.1",
    "@wdio/spec-reporter": "^8.31.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "del-cli": "^5.1.0",
    "path": "^0.12.7"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "clean": "npx del test-report",
    "report:generate": "npx allure generate --clean ./test-report/allure-result/ -o ./test-report/allure-report",
    "report:open": "npx allure open test-report/allure-report",
    "wdio-local": "npm-run-all clean wdio report:generate report:open --continue-on-error"
  },
  "dependencies": {
    "allure-commandline": "^2.27.0",
    "dotenv": "^16.4.4",
    "npm-run-all": "^4.1.5",
    "winston": "^3.11.0"
  }
}
