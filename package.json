{
  "name": "wdio-project",
  "author": {"name": "Hardik Chotaliya"},
  "version": "9.14.0",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.14.0",
    "@wdio/cli": "^9.14.0",
    "@wdio/cucumber-framework": "^9.14.0",
    "@wdio/local-runner": "^9.14.0",
    "@wdio/spec-reporter": "^9.14.0",
    "del-cli": "^6.0.0",
    "path": "^0.12.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "clean": "npx del test-report",
    "report:generate": "npx allure generate --clean ./test-report/allure-result/ -o ./test-report/allure-report",
    "report:open": "npx allure open test-report/allure-report",
    "wdio-local": "npm-run-all clean wdio report:generate report:open --continue-on-error"
  },
  "dependencies": {
    "allure-commandline": "^2.34.0",
    "chai": "^5.2.0",
    "dotenv": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "winston": "^3.17.0"
  }
}
